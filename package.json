{
  "name": "styleguide-react",
  "version": "1.0.2",
  "description": "Styleguide for SMBC applications",
  "repository": "https://github.com/smbc-digital/stockport-styleguide",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run build",
    "build": "webpack",
    "build:prod": "webpack --config webpack.config.prod.js",
    "start": "webpack && webpack-dev-server --content-base src --hot --inline",
    "lint:sass": "sass-lint -v",
    "lint:js": "eslint src/**/*.js"
  },
  "pre-commit": [
    "lint:sass",
    "lint:js"
  ],
  "keywords": [],
  "author": "Stockport Metropolitan Borough Council",
  "license": "MIT",
  "sasslintConfig": ".sass-lint.yml",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.9.0",
    "pre-commit": "^1.2.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.0",
    "smbc-react-components": "^1.6.24",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "pretty": "^2.0.0",
    "react-highlight": "^0.12.0"
  }
}
